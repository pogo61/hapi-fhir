				<version>1.1-SNAPSHOT</version>
				<executions>
					<execution>
						<goals>
							<goal>generate-structures</goal>
						</goals>
					</execution>
				</executions>
				<configuration>
					<package>ca.uhn.fhir.model.dstu2</package>
					<version>dstu2</version>
