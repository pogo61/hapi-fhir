		        <groupId>org.apache.felix</groupId>
		        <artifactId>maven-bundle-plugin</artifactId>
		        <extensions>true</extensions>
		        <version>2.4.0</version>
		        <configuration>
		          <instructions>
		          	<Bundle-SymbolicName>hapi-fhir-structures-dstu2</Bundle-SymbolicName>
		            <Export-Package>
		            	ca.uhn.fhir.model.dstu2.*;version=1.1-SNAPSHOT,
		            	ca.uhn.fhir.rest.server.provider.dstu2.*;version=1.1-SNAPSHOT
		            </Export-Package>
		           	<Private-Package>
		           		COM.jrockit.reflect;resolution:=optional,
		           		jrockit.vm.*;resolution:=optional,
		           		com.ctc.wstx.*;resolution:=optional,
		           		groovy.lang.*;resolution:=optional,
		           		javax.annotation.concurrent.*;resolution:=optional,
		           		javax.json.*;resolution:=optional,
		           		ognl.*;resolution:=optional,
		           		org.apache.commons.*;resolution:=optional,
